{
  "aliases": [
    "bjshare-irc",
    "bjshare",
    "bjs",
    "bj"
  ],
  "variables": [
    {
      "name": "year",
      "from": null,
      "regex": ".*\\[(\\d{4})\\].*",
      "replaceWith": "$1"
    },
    {
      "name": "rawTitle",
      "from": null,
      "regex": "\\[\\d{4}\\].*",
      "replaceWith": ""
    },
    {
      "name": "title",
      "from": "rawTitle",
      "regex": ".*?\\[(.*?)(\\[.*\\])?\\].*",
      "replaceWith": "$1"
    },
    {
      "name": "rawMeta",
      "from": null,
      "regex": ".*\\[\\d{4}\\](.*)",
      "replaceWith": "$1"
    },
    {
      "name": "meta",
      "from": "rawMeta",
      "regex": "Dual .udio",
      "replaceWith": "Dual Audio Original Brazilian"
    },
    {
      "name": "meta",
      "from": "meta",
      "regex": "(Legendado|Nacional)",
      "replaceWith": "Original Audio $1"
    },
    {
      "name": "meta",
      "from": "meta",
      "regex": "Full HD",
      "replaceWith": "1080p"
    },
    {
      "name": "meta",
      "from": "meta",
      "regex": "4K",
      "replaceWith": "2160p"
    },
    {
      "name": "meta",
      "from": "meta",
      "regex": "SD",
      "replaceWith": "480p"
    },
    {
      "name": "meta",
      "from": "meta",
      "regex": "/ HD]",
      "replaceWith": "720p"
    },
    {
      "name": "meta",
      "from": "meta",
      "regex": "/ HD /",
      "replaceWith": "720p"
    },
    {
      "name": "meta",
      "from": "meta",
      "regex": "[\\[\\]]+",
      "replaceWith": ""
    }
  ],
  "output": "${title} [${year}] ${meta}"
}